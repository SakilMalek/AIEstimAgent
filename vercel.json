{
  "buildCommand": "vite build --outDir dist",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://aiestimagent-api.onrender.com",
    "VITE_ML_URL": "https://aiestimagent.onrender.com"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
