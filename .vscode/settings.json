{
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "schema": {
        "type": "object"
      }
    }
  ],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "tailwindCSS.includeLanguages": {
    "typescript": "typescript",
    "javascript": "javascript",
    "typescriptreact": "typescriptreact",
    "javascriptreact": "javascriptreact"
  },
  "editor.quickSuggestions": {
    "strings": true
  }
}
